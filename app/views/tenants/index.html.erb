<h2>Borrowers</h2>

<div class="pagination">
  <%= will_paginate @tenants %>
  <% @tenants.each do |tenant| %>
    <% cache tenant do %>
      <div class="borrower">
        <p class="tenant"><%= link_to "#{tenant.name}", tenant_path(tenant.slug) %></p>
        <p><%= tenant.blurb %></p>
        <p><%= link_to "Learn more about this borrower", tenant_path(tenant.slug) %></p>
      </div>
    <% end %>
  <% end %>
</div>

<div style="clear:both;"></div>
